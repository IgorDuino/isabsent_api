{
    "version": 1,
    "formatters": {"default": {
        "format": "[%(asctime)s] %(levelname)s in %(module)s: %(message)s"
    }},
    "handlers": {"wsgi": {
        "class": "logging.FileHandler",
        "filename": "app.log",
        "formatter": "default",
        "mode": "a"
    }},
    "root": {
        "level": "warning",
        "handlers": ["wsgi"]
    }
}